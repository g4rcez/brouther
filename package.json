{
  "name": "brouther",
  "version": "4.1.3",
  "main": "./dist/brouther.mjs",
  "module": "./dist/brouther.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/brouther.mjs",
      "require": "./dist/brouther.mjs",
      "type": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build; tsc",
    "dev": "vite build --watch",
    "pack": "npm pack --dry-run --pack-destination out out/brouther",
    "prepack": "pnpm build",
    "prepare": "vite build",
    "start": "vite build --watch",
    "test": "vitest run && npm run test:types",
    "test:types": "tsc -p ./tsconfig.test.json",
    "test:watch": "vitest watch",
    "cy": "cypress open",
    "cy:run": "cypress run",
    "cy:browser": "start-server-and-test start http://localhost:5173 cy:run",
    "typedoc": "typedoc --out docs src/index.ts"
  },
  "dependencies": {
    "history": "5.3.0",
    "qs": "6.11.1",
    "ts-toolbelt": "9.6.0"
  },
  "peerDependencies": {
    "react": ">=16.8.3"
  },
  "devDependencies": {
    "@types/node": "18.11.19",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.27",
    "cypress": "12.10.0",
    "prettier": "2.8.8",
    "start-server-and-test": "2.0.0",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "vite": "4.3.1",
    "vitest": "0.30.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "volta": {
    "node": "18.13.0"
  }
}
